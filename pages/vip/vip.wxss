/* vip.wxss */
page {
  position: relative;
  width: 100%;
  height: 100%;
  background: #ebebeb;
}

.page_container {
  display: flex;
  flex:1;
  flex-direction: column;
}

.location_container {
  display:flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
  margin-left: 2%;
  margin-right: 2%;
  border-radius: 6rpx;
  width:96%;
  height: 200rpx;
  background: #ffffff;
}


button {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 16px;
  border: 0 none;
  background: #09bb07;
  color: #fff;
  margin: 30rpx auto;
}